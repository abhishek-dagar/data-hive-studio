{
  "name": "data-hive-studio",
  "version": "0.1.0-alpha",
  "private": true,
  "main": "build/main.js",
  "description": "Data Hive",
  "type": "module",
  "scripts": {
    "dev:next": "next dev -p 4080",
    "dev": "cross-env NODE_ENV=development concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev -p 4080\" \"electron .\"",
    "clean": "rm -rf build dist .tscache .next out",
    "build": "yarn clean && yarn build:next && yarn build:ts && yarn build:electron",
    "build:next": "next build",
    "build:ts": "tsc --project tsconfig-electron.json && node scripts/rename-preload-script.js",
    "build:electron": "electron-builder --config electron-builder.yml",
    "start:next": "next start -p 4080",
    "start:ts": "tsc-watch --noClear --onFirstSuccess \"yarn start:electron\" --project tsconfig-electron.json",
    "start:electron": "electron .",
    "start:dev": "cross-env NODE_ENV=development concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"yarn dev:next\" \"yarn start:ts\"",
    "open": "./dist/mac-arm64/Next\\ Electron\\ RSC.app/Contents/MacOS/Next\\ Electron\\ RSC",
    "format": "prettier --write ."
  },
